{
  "_version": "1.12.0",
  "sap.app": {
    "id": "lp.fscomposer",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "sourceTemplate": {
      "id": "ui5template.basicSAPUI5ApplicationProject",
      "version": "1.40.12"
    },
    "settings": {
      "flexibleColumnLayout": {
        "defaultTwoColumnLayoutType": "TwoColumnsMidExpanded",
        "defaultThreeColumnLayoutType": "ThreeColumnsEndExpanded"
      }
    }
  },

  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },

  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "lp.fscomposer.view.App",
      "type": "XML",
      "async": true,
      "id": "MainView"
    },
    "dependencies": {
      "minUI5Version": "1.60.1",
      "libs": {
        "sap.ui.layout": {},
        "sap.ui.core": {},
        "sap.m": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "lp.fscomposer.i18n.i18n"
        }
      },
      "app": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {},
        "uri": "model/app.json",
        "preload": false
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "lp.fscomposer.view",
        "controlId": "idAppControl",
        "transition": "slide",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "TargetBusinessPartnerList",
          "pattern": "",
          "layout": "ThreeColumnsEndExpanded",
          "target": ["Master", "FSPreview", "FoldingSheet"]
        },
        {
          "name": "TargetBusinessPartnerDetail",
          "pattern": "/preview/{foldingSheetGroupId}",
          "layout": "ThreeColumnsEndExpanded",
          "target": ["Master", "FSPreview", "FoldingSheet"]
        },
        {
          "name": "TargetBusinessPartnerSalesOrderDetail",
          "pattern": "/preview/{foldingSheetGroupId}/foldingSheet/{foldingSheetId}",
          "layout": "ThreeColumnsEndExpanded",
          "target": ["Master", "FSPreview", "FoldingSheet"]
        }
      ],
      "targets": {
        "Master": {
          "controlAggregation": "beginColumnPages",
          "viewName": "Master"
        },
        "FSPreview": {
          "controlAggregation": "midColumnPages",
          "viewName": "FSPreview"
        },
        "FoldingSheet": {
          "controlAggregation": "endColumnPages",
          "viewName": "FoldingSheet"
        }
      }
    }
  }
}
