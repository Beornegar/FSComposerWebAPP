                busyIndicatorDelay="500"
                enableBusyIndicator="true"
                noDataText="No products available"
                items="{viewData>/PuzzelGroup}">
                <headerToolbar>
                    <OverflowToolbar>
                        <Title text="Groups" level="H2" />
                    </OverflowToolbar>
                </headerToolbar>
                <StandardListItem title="{viewData>Name}"/>
            </List>
